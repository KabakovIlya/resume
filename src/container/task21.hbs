<div>
    {{#with heading}}
    <div>
        <div class="card">
            <div class="card-body">

                <div class="card">
                    <div class="card-body">
                        {{#with main}}
                        <h1>{{title}}</h1>
                        <div><em>{{text}}</em></div>
                        <br>
                        {{#with button_text}}
                        <div>
                            <button class="btn btn-primary">{{solid}}</button>
                        </div>

                        <br>
                        <div>
                            <button class="btn btn-outline-primary">{{outline}}</button>
                        </div>
                        {{/with}}
                    </div>
                </div>
                {{/with}}

                <br>

                <div class="card">
                    <div class="card-body">
                        {{#with second}}
                        <h2>{{title}}</h2>
                        <div>{{text}}</div>
                        <br>
                        {{#with button_text}}
                        <div>
                            <button class="btn btn-secondary">{{solid}}</button>
                        </div>
                        <br>
                        <div>
                            <button class="btn btn-outline-secondary">{{outline}}</button>
                        </div>
                        {{/with}}
                        {{/with}}
                    </div>
                </div>

                <br>

                <div class="card">
                    <div class="card-body">
                        {{#with subblock}}
                        <h3>{{title}}</h3>
                        <div>{{text}}</div>
                        <br>
                        {{#with button_text}}
                        <div>
                            <button class="btn btn-dark">{{solid}}</button>
                        </div>
                        <br>
                        <div>
                            <button class="btn btn-outline-dark">{{outline}}</button>
                        </div>
                        {{/with}}
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{/with}}
</div>

<br>

<div>
    {{#with nested_display}}
    <div class="card">
        <div class="card-body">
            <div class="display-1">{{title}}</div>
            <br>
            {{#with children_display}}
            <div class="card">
                <div class="card-body">
                    <div class="display-4">{{title}}</div>
                    <div>{{text}}</div>
                    <br>
                    {{#with button}}
                    <div>
                        <button class="btn btn-outline-warning">{{text}}</button>
                    </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}

            <br>

            <div>
                <h6>{{header_text}}</h6>
                {{#with button}}
                <button class="btn btn-info">{{text}}</button>
                {{/with}}
            </div>
        </div>
    </div>
    {{/with}}
</div>

<br>

<div>
    {{#with paragraph}}
    <div class="card">
        <div class="card-body">
            <div class="text-uppercase">{{title}}</div>
            <br>
            <div class="text-lowercase">{{text}}</div>
            <br>
            {{#with button}}
            <button class="btn btn-link">
                <a href="{{url}}" target=" _blank">{{text}}</a>
            </button>
            {{/with}}

            <br>

            <div class="card">
                <div class="card-body">
                    <ul>
                        {{#each list_name}}
                        <li>
                            <button class="btn btn-link">
                                <a href="{{url}}" target="blank">{{text}}</a>
                            </button>
                        </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
    </div>
    {{/with}}
</div>